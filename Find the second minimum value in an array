#include <stdio.h>
#include <limits.h> 
int main() {
    int arr[] = {12, 45, 3, 22, 18, 5};
    int n = sizeof(arr) ;

    int first = INT_MAX, second = INT_MAX;

    for (int i = 0; i < n; i++) {
        if (arr[i] < first) {
            second = first;
            first = arr[i];
        } else if (arr[i] < second && arr[i] != first) {
            second = arr[i];
        }
    }
